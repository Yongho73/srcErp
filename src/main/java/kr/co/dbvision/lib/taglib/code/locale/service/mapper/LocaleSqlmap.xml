<mapper namespace="kr.co.dbvision.lib.taglib.code.locale.service.mapper.LocaleMapper">
    <select id="selectLocale" parameterType="egovMapForNull" resultType="egovMapForNull">
        /* kr.co.dbvision.lib.taglib.code.locale.service.impl.LocaleMpper.selectLocaleCode */
